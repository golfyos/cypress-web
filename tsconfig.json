{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es6", "dom"],
      "types": ["cypress", "node"],
      "noImplicitAny": true,
      "baseUrl": ".",
      "noEmit": true,
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "include": ["**/*.ts"]
}
